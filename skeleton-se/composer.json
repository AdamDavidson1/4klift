{
    "name": "deasilworks/4klift-se",
    "description": "Big Data Framework",
    "type": "project",
    "license": "MIT",
    "keywords": ["framework","cassandra","php","4klift","cms","api"],
    "authors": [
        {
            "name": "Deasil Works, Inc.",
            "email": "code@deasil.works"
        },
        {
            "name": "Craig Johnston",
            "email": "craig@deasil.works"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "deasilworks/4klift-core": "dev-master"
    },
    "autoload": {
        "psr-0": { "MyProject": "src/" }
    },
    "scripts": {
        "link-core": [
            "ln -sf ./vendor/deasilworks/4klift-core/Vagrantfile ./",
            "ln -sf ../vendor/deasilworks/4klift-core/dev ./dev",
            "ln -sf ../vendor/deasilworks/4klift-core/index.php ./web",
            "ln -sf ../vendor/deasilworks/4klift-core/4klift.php ./core",
            "ln -sf ../vendor/deasilworks/4klift-core/cli.php ./core",
            "ln -sf ../vendor/deasilworks/4klift-core/cfg/example-application.yml ./cfg/application.yml",
            "ln -sf ../vendor/deasilworks/4klift-core/cfg/example-defaults.yml ./cfg/defaults.yml",
            "ln -sf ../vendor/deasilworks/4klift-core/cfg/example-parameters.yml ./cfg/parameters.yml"
        ],
        "post-create-project-cmd": [
            "@link-core"
        ]
    }
}
